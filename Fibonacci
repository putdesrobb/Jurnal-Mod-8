package fibonacci;

import java.util.Scanner;
public class Fibonacci {

    public static void main(String[] args) {
        int n;
        System.out.print("Masukkan Bilangan Fibonacci = ");
        Scanner in=new Scanner(System.in);
        n=in.nextInt();
        int jumlah=0;
        int f=0;
        int g=1;
        
        for(int i=0; i<=n; i++){
            System.out.print(f +" ");
            f=f+g;
            g=f-g;
            jumlah=jumlah+g;
        }
        System.out.println();
        System.out.println("Jumlah = "+jumlah);
    }
    
}
